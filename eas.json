{
  "cli": {
    "version": ">= 16.3.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
        "android": {
        "autoIncrement": false
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
        "android": {
        "autoIncrement": false
      }
    },
    "production": {
      "credentialsSource": "remote",
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "autoIncrement": "versionCode"
      },
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
